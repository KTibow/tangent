{
  "name": "tangent",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "upload": "deno run --allow-all --env-file scripts/upload.ts",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "lint-fix": "prettier --check . && eslint . --fix"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.29.0",
    "@iconify/types": "^2.0.0",
    "@ktibow/iconset-material-symbols": "^0.0.1750746019",
    "@ktibow/material-color-utilities-nightly": "^0.3.11750903831866",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tsndr/cloudflare-worker-jwt": "^3.2.0",
    "devalue": "^5.1.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-svelte": "^3.9.3",
    "fast-xml-parser": "^5.2.5",
    "globals": "^16.2.0",
    "kreations": "^1.2.5",
    "lean-qr": "^2.5.0",
    "m3-svelte": "^4.5.1",
    "pbf-esm": "^4.0.1",
    "prettier": "^3.6.1",
    "prettier-plugin-svelte": "^3.4.0",
    "school-districts": "^1.0.6",
    "svelte": "^5.34.8",
    "svelte-check": "^4.2.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^6.3.5",
    "vite-plugin-devtools-json": "^0.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
