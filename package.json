{
  "name": "tangent",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "upload": "deno run --allow-all --env-file scripts/upload.ts",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "deno run --allow-read --allow-env --allow-run npm:oxlint && prettier --check . && eslint .",
    "lint-fix": "prettier --check . && eslint . --fix"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.34.0",
    "@iconify/types": "^2.0.0",
    "@ktibow/iconset-material-symbols": "^0.0.1755063979",
    "@ktibow/material-color-utilities-nightly": "^0.3.11756343896616",
    "@mercuryworkshop/bare-mux": "^2.1.7",
    "@sveltejs/adapter-node": "^5.3.1",
    "@sveltejs/kit": "^2.36.3",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@tsndr/cloudflare-worker-jwt": "^3.2.0",
    "devalue": "^5.3.2",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-svelte": "^3.11.0",
    "fast-xml-parser": "^5.2.5",
    "globals": "^16.3.0",
    "kreations": "^1.2.5",
    "lean-qr": "^2.5.0",
    "m3-svelte": "^5.4.0",
    "oxlint": "^1.13.0",
    "pbf-esm": "^4.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "rolldown": "1.0.0-beta.34",
    "school-districts": "^1.0.6",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3",
    "vite-plugin-devtools-json": "^1.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
